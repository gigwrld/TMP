@startuml

class пользователь {
+ ФИО
+ эл.почта
+ номер телефона
+ пол
+ дата рождения
+ отправить сообщение ()
+ получить сообщение ()
+ отправить запрос в систему на аннулирвоние/создание аккаунта ()
}

class сообщение {
+ длина
+ содержание 
+ автор
}

class запрос {
+ ФИО пользователя
+ никнейм
+ логин
- пароль
}

class аккаунт {
+ никнейм
+ ФИО пользователя 
+ логин
- пароль
}

class admin {
- статус
- полномочия
+ обработка запроса ()
+ регистрирвоание аккаунта ()
+ аннулирование аккаунта ()
}
