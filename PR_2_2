@startuml

class пользователь {
+ ФИО
+ эл.почта
+ номер телефона
+ пол
+ дата рождения
+ отправить сообщение ()
+ получить сообщение ()
+ отправить запрос в систему на аннулирование/создание аккаунта ()
}

class сообщение {
+ длина
+ содержание 
+ автор
}

class запрос {
+ ФИО пользователя
+ никнейм
+ логин
- пароль
}

class аккаунт {
+ никнейм
+ ФИО пользователя 
+ логин
- пароль
}

class admin {
- статус
- полномочия
+ обработка запроса ()
+ регистрирование аккаунта ()
+ аннулирование аккаунта ()
}

admin <.. запрос
пользователь <.. запрос
пользователь <.. сообщение
admin  --|> аккаунт
пользователь --o аккаунт

@enduml
